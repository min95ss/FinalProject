				inviteMail : inviteMail.value,
				inviteContent : inviteContent.value,
				invitePjName : invitePjName.value
			},
			success : function(result){
				$('#inviteModal').modal('hide');
				$('#inviteMail').val('');
				$('#inviteContent').val('');
			}
		});
	});	
	
