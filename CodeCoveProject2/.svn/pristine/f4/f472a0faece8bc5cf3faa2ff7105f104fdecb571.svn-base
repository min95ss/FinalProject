	<select id="selectDownload" parameterType="String"
		resultType="AttatchVO">
		SELECT *
		FROM TB_ATTATCH
		WHERE ATTATCH_NUM = #{attatchNum}
	</select>

	<select id="selectAttatchFile" parameterType="AttatchVO"
		resultType="AttatchVO">
		SELECT *
