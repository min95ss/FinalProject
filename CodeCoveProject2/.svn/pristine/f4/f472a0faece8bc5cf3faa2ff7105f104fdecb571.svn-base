	<update id="modifyIssueReplyAttatch" parameterType="AttatchVO">
		UPDATE TB_ATTATCH
		SET
		FILE_PATH=#{filePath},
		ORIGIN_NM=#{originNm},
		SAVE_NM=#{saveNm},
		ATTATCH_TYPE=#{attatchType},
		ATTATCH_SIZE=#{attatchSize}
        WHERE ATTATCH_NUM=#{attatchNum}
	</update>
	
