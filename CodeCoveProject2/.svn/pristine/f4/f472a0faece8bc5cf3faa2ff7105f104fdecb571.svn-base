	<!-- insertQnAAttatches -->
	<insert id="insertQnAAttatches" parameterType="qnaVO">
		<selectKey keyProperty="attatchNum, qnaNum" resultType="qnaVO" order="BEFORE">
			<if test="@org.apache.commons.lang3.StringUtils@isBlank(qnaNum)">
				SELECT
					SELECT
						  FN_CREATE_NUM(TO_CHAR(SYSDATE,'YYMMDD'), 'TB_ATTATCH', 'ATTATCH_NUM', UPPER('ATTA')) FROM DUAL) AS ATTATCH_NUM
						, ( SELECT
								FN_CREATE_NUM(TO_CHAR(SYSDATE,'YYMMDD'), 'TB_QNA', 'QNA_NUM', UPPER('QNAA'))
							FROM
								DAUL ) AS QNA_NUM
				FROM DUAL
				
			</if>
			<if test="@org.apache.commons.lang3.StringUtils@isBlank(qnaNum)">
				SELECT
					  ( SELECT 
							  FN_CREATE_NUM(TO_CHAR(SYSDATE,'YYMMDD'), 'TB_ATTATCH', 'ATTATCH_NUM', UPPER('ATTA')) 
						FROM DAUL ) AS ATTATCH_NUM
					, ( (SELECT 
							QNA_NUM 
						 FROM 
						 	TB_QNA 
						 WHERE 
						 	TB_QNA=#{qnaNum}) AS QNA_NUM 
				FROM
					DAUL
			</if>
		</selectKey>
		INSERT ALL
		<foreach collection="attatchList" index="idx" item="attatch">
			INTO TB_ATTATCH (
				ATTATCH_NUM
			  , ATTATCH_ORDER
			  , ATTATCH_PLACE
			  , FILE_PATH
			  , ORIGIN_NM
			  , SAVE_NM
			  , ATTATCH_TYPE
			  , ATTATCH_SIZE
			  , UPLOADER_ID
				) VALUES (
			  #{attatchNum}
			  , #{idx}
			  , #{freeNum}
			  , #{attatch.filePath,jdbcType=VARCHAR}
			  , #{attatch.originNm,jdbcType=VARCHAR}
			  , #{attatch.saveNm,jdbcType=VARCHAR}
			  , #{attatch.attatchType,jdbcType=VARCHAR}
			  , #{attatch.attatchSize,jdbcType=NUMERIC}
			  , #{writerId,jdbcType=VARCHAR})
		</foreach>
		SELECT * FROM DUAL	
	</insert>


	<!-- freeBoardAttatch -->
	<insert parameterType="FreeBoardVO"
		id="insertFreeBoardAttatches">
		<selectKey keyProperty="attatchNum,freeNum"
			resultType="FreeBoardVO" order="BEFORE">
			<if test="@org.apache.commons.lang3.StringUtils@isBlank(freeNum)">
				SELECT
				(SELECT
				FN_CREATE_NUM(TO_CHAR(SYSDATE,'YYMMDD'), 'TB_ATTATCH', 'ATTATCH_NUM', UPPER('ATTA'))
				FROM DUAL) AS ATTATCH_NUM
				, (SELECT FN_CREATE_NUM(TO_CHAR(SYSDATE,'YYMMDD'), 'TB_FREEBOARD',
				'FREE_NUM', UPPER('FREE'))
				FROM DUAL) AS FREE_NUM
				FROM DUAL
			</if>
			<if
				test="@org.apache.commons.lang3.StringUtils@isNotBlank(freeNum)">
				SELECT
				(SELECT
				FN_CREATE_NUM(TO_CHAR(SYSDATE,'YYMMDD'), 'TB_ATTATCH', 'ATTATCH_NUM', UPPER('ATTA'))
				FROM DUAL) AS ATTATCH_NUM
				, (SELECT FREE_NUM FROM TB_FREEBOARD WHERE FREE_NUM=#{freeNum}) AS
				FREE_NUM
				FROM DUAL
			</if>
		</selectKey>
		INSERT ALL
		<foreach index="idx" item="attatch" collection="attatchList">
			INTO TB_ATTATCH (
			ATTATCH_NUM
			, ATTATCH_ORDER
			, ATTATCH_PLACE
			, FILE_PATH
			, ORIGIN_NM
			, SAVE_NM
			, ATTATCH_TYPE
			, ATTATCH_SIZE
			, UPLOADER_ID
			) VALUES (
			#{attatchNum}
			, #{idx}
			, #{freeNum}
			,
			#{attatch.filePath,jdbcType=VARCHAR}
			,
			#{attatch.originNm,jdbcType=VARCHAR}
			,
			#{attatch.saveNm,jdbcType=VARCHAR}
			,
			#{attatch.attatchType,jdbcType=VARCHAR}
			, #{attatch.attatchSize,jdbcType=NUMERIC}
			, #{writerId,jdbcType=VARCHAR})
		</foreach>
		SELECT * FROM DUAL
	</insert>
